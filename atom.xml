---
layout: nil
---

<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Caleb Perkins</title>
	<link href="http://www.calebperkins.com/atom.xml" rel="self" />
	<link href="http://www.calebperkins.com/" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://www.calebperkins.com/</id>
	<author>
		<name>Caleb Perkins</name>
		<email>calebtperkins@gmail.com</email>
	</author>
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://www.calebperkins.com{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://www.calebperkins.com{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
